{
  "source": {
    "serverUrl": "",
    "siteContentUrl": "",
    "accessTokenName": ""
  },
  "destination": {
    "serverUrl": "",
    "siteContentUrl": "",
    "accessTokenName": ""
  },
  "tableau": {
    "migrationSdk": {
      "batchSize": 50
    },
    "migrationOptions": {
      "baseOverrideMailAddress": ""
    }
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "MinimumLevel": {
      "Default": "Information"
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "restrictedToMinimumLevel": "Warning",
          "outputTemplate": "[{Timestamp:HH:mm:ss.fff zzz} {Level:u3} {SourceContext}]\t{Message:lj}{NewLine}{Exception}"
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "logs/Tableau.Migration.TestApplication-.log",
          "rollingInterval": "Day",
          "retainedFileCountLimit": 15,
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz}|{Level:u3}|{SourceContext} -\t{Message:lj}{NewLine}{Exception}"
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName" ],
    "Properties": {
      "Application": "Tableau.Migration.TestApplication"
    }
  }
}
